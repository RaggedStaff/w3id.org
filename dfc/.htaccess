# Rewrite engine setup
RewriteEngine on


# Rewrite rule for ontology folder
RewriteRule /dfc/ontology/(.*)$  https://docs.datafoodconsortium.org/ontology/src/$1 [R=302,L]

# Rewrite rule for taxonomies folder
RewriteRule /dfc/taxonomies/(.*)$ https://docs.datafoodconsortium.org/taxonomies/src/$1 [R=302,L]
