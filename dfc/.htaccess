# Rewrite engine setup
RewriteEngine on

# Rewrite rule for latest ontology release.
RewriteRule ^ontology/latest/(.*)  https://cdn.jsdelivr.net/gh/datafoodconsortium/ontology@latest/$1 [R=302,L]

# Rewrite rule for versioned ontology folders
RewriteRule ^ontology/v([0-9]+.[0-9]+.[0-9]+)/(.*)  https://cdn.jsdelivr.net/gh/datafoodconsortium/ontology@v$1/$2 [R=302,L]

# Rewrite rule for versioned taxonomies releases
RewriteRule ^taxonomies/v([0-9]+.[0-9]+.[0-9]+)/(.*)  https://cdn.jsdelivr.net/gh/datafoodconsortium/taxonomies@v$1/$2 [R=302,L]

# Rewrite rule for latest taxonomies folder
RewriteRule ^taxonomies/latest/(.*) https://cdn.jsdelivr.net/gh/datafoodconsortium/taxonomies@latest/$1 [R=302,L]
